{
  "search.exclude": {
    "client/dist": true
  },
  "files.exclude": {
    "obj": true,
    "bin": true
  },
  "appService.advancedCreation": false,
  "appService.showBuildDuringDeployPrompt": true,

  "appService.zipIgnorePattern": [
    "node_modules{,/**}",
    ".vscode{,/**}",
    "util{,/**}",
    "client/html{,/**}",
    "client/img{,/**}",
    "client/src{,/**}",
    "client/jsconfig.json",
    "docker**",
    ".dockerignore",
    ".git*",
    ".babelrc",
    ".eslintrc.json",
    ".env",
    "gulpfile.js"
  ],
  "appService.deploySubpath": ""
}
